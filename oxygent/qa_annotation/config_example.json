{
  "default": {
    "qa_annotation": {
      "enabled": true,
      "realtime_hook_enabled": false,
      
      "mq": {
        "type": "redis",
        "redis": {
          "stream_prefix": "qa",
          "consumer_group": "qa_processor"
        }
      },
      
      "collector": {
        "exclude_callees": ["retrieve_tools", "default_llm"],
        "exclude_callee_types": ["llm"],
        "min_question_length": 2,
        "min_answer_length": 10
      },
      
      "llm_processor": {
        "enabled": false,
        "model": "default_llm",
        "quality_threshold": 0.3
      },
      
      "task": {
        "expire_hours": 24,
        "dedup_enabled": true,
        "priority_weights": {
          "e2e": 0,
          "user_agent": 1,
          "agent_agent": 2,
          "agent_tool": 3
        }
      },
      
      "review": {
        "enabled": true,
        "require_review_for_kb": true
      },
      
      "platform": {
        "page_size": 20,
        "enable_kb_export": false
      }
    }
  }
}
